<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GrupoPrueba</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/ejemplopagina.css">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">GrupoPrueba</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark cat">Catalogo</a>
                        </li>
                        
                        
                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class='categoria'>
            <ul class='cat-ul'>
                <li class = "cat-li"><a  asp-controller="Catalogo" asp-action="Index">Procesadores</a></li>
                <li class = "cat-li"><a asp-controller="Catalogo" asp-action="Tarjeta">Tarjetas Graficas</a></li>
                <li class = "cat-li"><a asp-controller="Catalogo" asp-action="Memoria">Memoria ram</a></li>
                <li class = "cat-li"><a asp-controller="Catalogo" asp-action="Fuente">Fuente</a></li>
                <li class = "cat-li"><a asp-controller="Catalogo" asp-action="Disco">Disco Duro</a></li>
                <li class = "cat-li"><a asp-controller="Catalogo" asp-action="SSD">.SSD</a></li>
                <li class = "cat-li"><a asp-controller="Catalogo" asp-action="Case">Case</a></li>
                <li class = "cat-li"><a asp-controller="Catalogo" asp-action="Placa">Placa Madre</a></li>

            </ul>
        </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - GrupoPrueba - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/app.js">
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
